#include<bits/stdc++.h>
using namespace std;
struct node{
int val;
node * left, *right;
node(int x): val(x),left(NULL), right(NULL){}
};
int pathsum(node *curr, int K){
if(curr==NULL){
    return 0;
}
if(curr->left == NULL && curr->right==NULL){
    if(curr->val==K) return 1;
    return 0;
}
return pathsum(curr->left,K-(curr->val))
+pathsum(curr->right,K-(curr->val));
}
int main(){
    return 0;
}